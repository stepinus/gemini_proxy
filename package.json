{
  "name": "gemini-proxy-api",
  "version": "1.0.0",
  "description": "OpenAI compatible API proxy for Google Gemini",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@google/genai": "^0.3.0",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}